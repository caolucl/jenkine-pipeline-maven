#!groovy

pipeline {
    agent any

    tools {
        maven "maven3.5.4" // You need to add a maven with name "maven3.5.4" in the Global Tools Configuration page
    }

    stages {
        stage("Build") {
            steps {
                sh "mvn -version"
                sh "mvn clean install"
            }
        }
    }

    #Delete workspace
    #post {
    #    always {
    #        cleanWs()
    #    }
    #}
}
